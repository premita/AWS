<scheduledTasks xmlns="http://xmlns.oracle.com/oim/scheduler">
    <task>
        <name>Organization Reconciliation</name>
        <class>com.icsynergy.awsproject.OrgReconTask</class>
        <description>Reconciles Organization</description>
        <retry>0</retry>
        <parameters>
            <string-param required="true" helpText="Name of a view to pull a data from">Table Name</string-param>
            <string-param required="false" helpText="Group IDs to filter out with | as a separator">ID List To Filter Out</string-param>
        </parameters>
    </task>
    <task>
        <name>Org-Venue Lookup Reconciliation</name>
        <class>com.icsynergy.awsproject.OrgVenueLookupReconTask</class>
        <description>Reconciles Organization-Venue relationship into a lookup</description>
        <retry>0</retry>
        <parameters>
            <string-param required="true" helpText="Name of a lookup to populate values into">Lookup Name</string-param>
        </parameters>
    </task>
</scheduledTasks>

